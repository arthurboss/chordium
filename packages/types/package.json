{
  "name": "@chordium/types",
  "version": "1.0.0",
  "description": "TypeScript types and interfaces for Chordium",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "clean": "rm -rf dist"
  },
  "devDependencies": {
    "typescript": "^5.8.3",
    "tsup": "^7.0.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
